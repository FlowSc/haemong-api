# QA Environment Configuration
NODE_ENV=qa
PORT=3000

# Logging
LOG_LEVEL=info

# Database (Supabase - QA)
SUPABASE_URL=https://your-qa-project.supabase.co
SUPABASE_ANON_KEY=your_qa_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_qa_service_role_key

# JWT Configuration
JWT_SECRET=qa_jwt_secret_key_change_in_production
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=qa_jwt_refresh_secret_key_change_in_production
JWT_REFRESH_EXPIRES_IN=7d

# OAuth Configuration (QA)
GOOGLE_CLIENT_ID=your_qa_google_client_id
GOOGLE_CLIENT_SECRET=your_qa_google_client_secret
APPLE_CLIENT_ID=your_qa_apple_client_id
APPLE_TEAM_ID=your_apple_team_id
APPLE_KEY_ID=your_apple_key_id
APPLE_PRIVATE_KEY_PATH=./keys/qa-apple-private-key.p8

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-3.5-turbo

# Replicate API (Video Generation)
REPLICATE_API_TOKEN=your_replicate_api_token

# Frontend URL
FRONTEND_URL=https://your-qa-frontend.herokuapp.com

# Rate Limiting (Production-like but slightly more lenient)
RATE_LIMIT_SHORT_TTL=1000
RATE_LIMIT_SHORT_LIMIT=15
RATE_LIMIT_MEDIUM_TTL=60000
RATE_LIMIT_MEDIUM_LIMIT=150
RATE_LIMIT_LONG_TTL=900000
RATE_LIMIT_LONG_LIMIT=1500

# Security Settings
ENABLE_CORS=true
ENABLE_HELMET=true
ENABLE_VALIDATION=true

# Feature Flags
ENABLE_IMAGE_GENERATION=true
ENABLE_VIDEO_GENERATION=true
ENABLE_PREMIUM_FEATURES=true

# QA Specific Settings
QA_TEST_USER_EMAIL=qa-test@example.com
QA_ADMIN_EMAIL=qa-admin@example.com